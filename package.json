    "@finsweet/tsconfig": "^1.4.2",
    "@playwright/test": "^1.55.1",
    "cross-env": "^10.0.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1"
  },
  "dependencies": {
    "@finsweet/ts-utils": "^0.40.0",
